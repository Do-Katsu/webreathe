<form method="post" action="{{ path('app_modules_delete', {'id': module.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ module.id) }}">
    <div class="btn btn-danger my-2">
        <button class="btn">Delete</button>
    </div>
</form>
